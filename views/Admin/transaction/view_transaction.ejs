<div class="main-content">
  <section class="section">
    <div class="section-header">
      <h1>View Transaction</h1>
      <div class="section-header-breadcrumb">
        <div class="breadcrumb-item active"><a href="/Dashboard">Dashboard</a></div>
        <div class="breadcrumb-item"><a href='/transaction_list'>Transaction</a></div>
        <div class="breadcrumb-item">View Transaction</div>
      </div>
    </div>
    <div class="section-body">
      <form action="/admin/Update_Profile" method="post" enctype="multipart/form-data">
        <div class="row mt-sm-4 justify-content-center">
          <div class="col-12 col-md-12 col-lg-10 offset-lg-0">
            <div class="card">
              <div class="card-body">
                <div class="form-group ">
                  <label class="col-form-label text-md-right col-12 col-md-5 col-lg-3">Transaction By</label>
                  <div class="col-sm-12 col-md-12">
                    <input type="text" name="transaction_by" value="<%=faqData?.transaction_by?.name%>"
                      class="form-control" disabled>
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-form-label text-md-right col-12 col-md-3 col-lg-3">Transaction To</label>
                  <div class="col-sm-12 col-md-12">
                    <input type="text" name="transaction_to" value="<%=faqData?.transaction_to?.name%>"
                      class="form-control" disabled>
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-form-label text-md-right col-12 col-md-3 col-lg-3">Amount</label>
                  <div class="col-sm-12 col-md-12">
                    <input type="text" name="amount" value="<%=faqData?.amount%>" class="form-control" disabled>
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-form-label text-md-right col-12 col-md-3 col-lg-3">Date</label>
                  <div class="col-sm-12 col-md-12">
                    <input type="text" name="createdAt" value="<%=moment(faqData?.createdAt).format('DD-MM-YYYY')%>"
                      class="form-control" disabled>
                  </div>
                </div>
                <div class="form-group">
                  <label class="col-form-label text-md-right col-12 col-md-3 col-lg-3">Time</label>
                  <div class="col-sm-12 col-md-12">
                    <input type="text" name="createdAt" value="<%=moment(faqData?.createdAt).format('hh:mm a')%>"
                      class="form-control" disabled>
                  </div>
                </div>
                <div class="form-group">
                  <div class="col-sm-12 col-md-12 text-right mt-4">
                    <a href="/admin/transaction_list" class="btn btn-info">Back</a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </form>
    </div>
  </section>
</div>


<script>
  imgInp.onchange = evt => {
    const [file] = imgInp.files
    if (file) {
      blah.src = URL.createObjectURL(file)
    }
  }
</script>