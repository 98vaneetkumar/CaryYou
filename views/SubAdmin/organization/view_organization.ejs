<div class="main-content">
  <section class="section">
    <div class="section-header">
      <h1>View Organization</h1>
      <div class="section-header-breadcrumb">
        <a href="/subAdmin/employee/<%=cateData._id%>" class="btn btn-sm " style="background-color: var(--theme);"><i
            class="fa fa-users"> Employee List </i></a>
        <!-- <div class="breadcrumb-item active"><a href="/subAdmin/dashboard">Dashboard</a></div>
        <div class="breadcrumb-item">View Organization</div> -->
      </div>
    </div>
    <div class="section-body">
      <div class="card">
        <!-- <form action="/subAdmin/organization" method="get" enctype="multipart/form-data"> -->
        <div class="row mt-sm-4 justify-content-center ">
          <div class="col-12 col-md-6 col-lg-4">
            <div class="profile-widget-header text-center py-3">
              <% if(cateData.image !='' ) { %>
                <img alt="" width="70%" height="70%" id="blah" src="<%=cateData.image%>"
                  class="profile_img rounded-circle">
                <% } else { %>
                  <img src="/assets/img/defaultimage.jpg" width="100%" height="100%" id="blah"
                    class="profile_img rounded-circle" alt="">
                  <% } %>
            </div>

          </div>
          <div class="col-12 col-lg-8 offset-lg-0">
            <form action="" method="post" class="needs-validation" enctype="multipart/form-data" novalidate="">
              <div class="card-body">
                <div class="form-group col-8 floating-addon">
                  <label> Organization Name</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <div class="input-group-text">
                        <i class="fas fa-user"></i>
                      </div>
                    </div>
                  </div>
                  <input type="text" class="form-control" value="<%=cateData.name%>" disabled>
                  <input type="hidden" name="id" value="">
                  <div class="invalid-feedback">
                    Please fill in the name
                  </div>
                </div>
                <!-- <div class="form-group col-8 floating-addon">
                  <label>Owner Name</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <div class="input-group-text">
                        <i class="fas fa-user"></i>
                      </div>
                    </div>
                  </div>
                  <input type="text" class="form-control" value="<%=cateData.ownerName%>" disabled>
                  <input type="hidden" name="id" value="">
                  <div class="invalid-feedback">
                    Please fill in the name
                  </div>
                </div> -->
                <div class="form-group col-8 floating-addon">
                  <label> Owner Email</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <div class="input-group-text">
                        <i class="fas fa-user"></i>
                      </div>
                    </div>
                  </div>
                  <input type="text" class="form-control" value="<%=cateData.email%>" disabled>
                  <input type="hidden" name="id" value="">
                  <div class="invalid-feedback">
                    Please fill in the name
                  </div>
                </div>
                <!-- <div class="form-group">
                  <div class="col-sm-12 col-md-5 text-right mt-4">
                    <button type="submit" class="btn btn-success">Back</button>
                  </div>
                </div> -->
              </div>
            </form>
          </div>
        </div>
        <!-- </form> -->
      </div>
      <div class="col-12 col-md-6 col-lg-12">
        <div class="section-header">
          <h1>Organization's Services</h1>
        </div>

        <div class="row">

        <% for(let i in cateData.service){ %>
            <div class="col-sm-6">
              <div class="card">
                <div class="row mt-sm-4 justify-content-center">
                  <div class="col-12 col-md-6 col-lg-4">
                    <div class="profile-widget-header text-center py-3">
                      <img alt="" width="150" height="150" id="blah" src="<%=cateData.service[i].image%>"
                        class="profile_img rounded-circle">
                    </div>
                  </div>
                </div>
                <div class="row justify-content-center">
                  <div class="col-sm-6 text-center">
                    <strong>
                      <%= cateData.service[i].service %>
                    </strong>
                  </div>
                </div>
              </div>
            </div>
          <% } %>
        </div>

      </div>
    </div>
  </section>
</div>

<script>
  $(document).ready(function () {
    $('#myTable').DataTable();
  });
  $(document).ready(function () {
    $('#myTable2').DataTable();
  });
  setTimeout(function () {
    $("div.msg").fadeOut();
  }, 2000);
</script>
<script>
  imgInp.onchange = evt => {
    const [file] = imgInp.files
    if (file) {
      blah.src = URL.createObjectURL(file)
    }
  }
</script>